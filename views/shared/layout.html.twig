    </head>
    <body>

    <!-- Container -->
    <div id="container">
        <!-- Header
            ================================================== -->
        <header class="clearfix">

            <div class="logo">
                <a href="/"><img src="/assets/images/logo.png" alt=""></a>
            </div>

            <nav class="nav-menu">
                <ul class="menu-list">
                    <li><a href="/">Home</a></li>
                    <li><a href="/projects">Our works</a></li>
                    <li><a href="/home/about-us">About Us</a></li>
                    <li><a href="/home/contact-us">Get in touch</a></li>
                </ul>
            </nav>

            <p>
                Â© 2019 Daxon. <br>All rights reserved. <br>
                Made by <a href="//hyper.starlight.co.zw" target="_blank"><span>hyper</span></a>
                from <a href="//starlight.co.zw" target="_blank"><span>Starlight</span></a>
            </p>

            <a class="menu-toggle" href="#"><i class="la la-list-ul"></i></a>

        </header>
        <!-- End Header -->

        <!-- switcher-box -->
        <div class="switcher-box">
            <ul class="switcher-list">
                {% if user %}
                    <li><a class="" href="/auth/profile"><i class="la la-user"></i> {{ user }}</a></li>
                {% else %}
                    <li><a class="" href="/auth/sign-in"><i class="la la-user"></i> Sign in</a></li>
                {% endif %}
                <li><a class="toggle-dark" href="#"><i class="la la-adjust"></i></a></li>
                <li><a class="toggle-shopping" href="#"><i class="la la-shopping-cart"></i> <span>2</span></a></li>
                <li><a class="toggle-sidebar" href="#"><i class="la la-reorder"></i></a></li>
            </ul>
        </div>
        <!-- end switcher-box -->

        <!-- content
            ================================================== -->
        <div id="content">
            {% block body %}{% endblock %}
        </div>
        <!-- End content -->

