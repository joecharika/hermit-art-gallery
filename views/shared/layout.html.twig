        <!-- fixed-sidebar-right -->
        <div id="fixed-sidebar">

            <div class="about-widget side-widget">
                <h2>What we do</h2>
                <p>We provide a set of digital marketing services tailored to fulfil your business goals.</p>
            </div>

            <div class="instagram-widget side-widget">
                <h2>Instagram Feed</h2>
                <ul class="instagram-list">
